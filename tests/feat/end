if { [isdraw result] } {
    #check if result is valid
    puts [checkshape result]
    
    clear
    smallview
    donly result
    fit
    xwd $imagedir/${test_image}.png 
} else {
    puts "Error : The feat grid cannot be built."
}

# to end a test script
puts "TEST COMPLETED"
