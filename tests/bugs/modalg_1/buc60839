
puts "============"
puts "BUC60839"
puts "============"
puts ""                                                                                                                                                                        

restore [locate_data_file BUC60839-1.brep] a 
checkshape a
restore [locate_data_file BUC60839-2.brep] b
checkshape b

puts "Starting fuse operation"

bfuse result a b

checkprops result -s 27341.6 
checkshape result
set 2dviewer 0
