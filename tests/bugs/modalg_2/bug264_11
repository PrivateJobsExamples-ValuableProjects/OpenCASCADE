puts "TODO OCC12345 ALL: Faulty OCC264_11: here is shading problem"
puts "TODO OCC12345 ALL: Error : The area of result shape is"

puts "========"
puts "OCC264"
puts " (case 11)"
puts "========"

restore [locate_data_file OCC264_11.brep] result
checkshape result

tclean result
vinit
vdisplay result
vclear

isos result 0
triangles result

set tri_info [trinfo result]
regexp { +([-0-9.+eE]+) +triangles} $tri_info full tri
regexp { +([-0-9.+eE]+) +nodes} $tri_info full nod

if { $tri != 32 && $nod != 28 }  {
    puts "Faulty OCC264_11: here is shading problem"
} else {
    puts "Shading of OCC264_11 is OK"
}

checkprops result -s 0 
set 3dviewer 0

